{
	"expo": {
		"name": "Recipe Book",
		"slug": "Recipe-Book",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/recipe-icon.png",
		"userInterfaceStyle": "light",
		"backgroundColor": "#24180f",
		"splash": {
			"image": "./assets/splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#ffffff"
		},
		"updates": {
			"fallbackToCacheTimeout": 0
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"supportsTablet": true
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/adaptive-icon.png",
				"backgroundColor": "#FFFFFF"
			},
			"package": "com.feras_ariabi.RecipeBook"
		},
		"web": {
			"favicon": "./assets/favicon.png"
		}
	}
}
